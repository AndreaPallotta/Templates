        "@utils/*": ["utils/*"],
        "@log/*": ["./logging/*"],
        "@mid/*": ["./middleware/*"],
        "@events/*": ["./events/*"],
        "@stores/*": ["./stores/*"]
      }
    },
    "exclude": ["node_modules", "build"]
  }